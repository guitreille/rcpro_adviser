<h1>Listing Simulations</h1>

<table>
  <thead>
    <tr>
      <th>coverage_ceiling</th>
      <th>deductible</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.coverage_ceiling %></td>
        <td><%= quote.deductible %></td>
        <td><% # link_to "Show", quote.id %></td>
        <td><% #link_to "Edit", edit_quote_path(quote) %></td>
        <td><%= link_to "Destroy", data: { turbo_method: :delete, turbo_confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New Simulation", new_simulation_path %>